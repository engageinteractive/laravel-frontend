{
    "name": "engageinteractive/laravel-frontend",
    "description": "Frontend Build Framework for Laravel Projects",
    "version": "3.0.2",
    "type": "library",
    "require": {
        "laravel/framework": ">=5.5 <6",
        "engageinteractive/laravel-config-provider": "^1.0",
        "engageinteractive/laravel-view-models": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "mockery/mockery": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "EngageInteractive\\LaravelFrontend\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Nadin",
            "email": "nadinengland@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "EngageInteractive\\LaravelFrontend\\ServiceProvider"
            ]
        }
    }
}
